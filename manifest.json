{
  "name": "Florida Man",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Replaces 'Trump' references with 'Florida Man'",
  "browser_action": {
    "default_title": "Florida Man",
    "default_icon": "icon48.jpg"
  },
  "icons": {
    "16": "icon16.jpg",
    "48": "icon48.jpg",
    "128": "icon128.jpg"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
  ],
  "content_scripts": [
    {
     "matches": ["https://*/*","http://*/*"],
     "js": ["inject.js"]
    }
  ]
}