{
  "name": "Florida Man",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Replaces 'Trump' references with 'Florida Man'",
  "browser_action": {
    "default_title": "Florida Man"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
  ],
  "content_scripts": [
    {
     "matches": ["https://*/*","http://*/*"],
     "js": ["inject.js"]
    }
  ]
}